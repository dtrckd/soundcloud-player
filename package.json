{
  "name": "soundcloud-player",
  "title": "Soundcloud Player",
  "id": "soundcloud-player@mikedeboer.nl",
  "description": "Control Soundcloud from your toolbar, with ease.",
  "author": "Mike de Boer <info@mikedeboer.nl>",
  "license": "MPL 2.0",
  "version": "0.10.0",
  "icon": "data/icon48x48.png",
  "icon64": "data/icon64x64.png",
  "minVersion": "29.*",
  "unpack": true,
  "main": "lib/main.js",
  "permissions": {
    "multiprocess": true
  },
  "preferences": [
    {
      "name": "useMediaKeys",
      "title": "Use Media Keys",
      "type": "bool",
      "value": false
    },
    {
      "name": "useRemoteControl",
      "title": "Use Apple Remote Control",
      "type": "bool",
      "value": false
    }
  ]
}
